<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SimpleTrader.WPF.Styles">
    <!--Colors-->
    <Color x:Key="ColorPrimaryOne">#799540</Color>
    <Color x:Key="ColorPrimaryTwo">#50632b</Color>
    
    <!--Brushes-->

    <SolidColorBrush x:Key="BrushPrimaryOne" Color="{StaticResource ColorPrimaryOne}"/>
    <SolidColorBrush x:Key="BrushPrimaryTwo" Color="{StaticResource ColorPrimaryTwo}"/>
</ResourceDictionary>